class EncapTest {
private String name;
private int age;

public String getName() {
return name;
}

public void setName(String newName) {
mame = newName;
}

public int getAge() {
return age;
}

public void setAge(int newAge) {
if (newAge > 30) {
age = 30;
} else {
age = newAge;
        }
    }
}

public class EncapTest {
public static void main(String[] args) {
    EncapTest encap = new EncapTest();
    encap.setName("James");
    encap.setAge(35);

    System.out.printim("Name : " + encap.getName());
    System.out.printin("Age : "+ encap.getAge());
}
public void setAge(int newAge){
    if (newAge > 30) {
        age = 30;
    } else if {newAge < 18}{
        age = 18;
    }else{
        age = newAge;
    }
}
